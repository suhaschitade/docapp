{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=docapp_db;Username=docapp_user;Password=your_secure_password_here"
  },
  "JwtSettings": {
    "SecretKey": "your-super-secret-jwt-key-here-minimum-256-bits-long",
    "Issuer": "DocApp",
    "Audience": "DocApp-Users",
    "ExpiryInMinutes": 60
  },
  "AllowedHosts": "*",
  "Urls": "http://127.0.0.1:5000",
  "Environment": "Production"
}
